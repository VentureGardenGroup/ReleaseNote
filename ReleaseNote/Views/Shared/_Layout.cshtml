<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Release Notes</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.css" rel="stylesheet"
          media="screen,projection">
    @Styles.Render("~/Content/materialize")
    @Styles.Render("~/Content/css")
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet" type="text/css">
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <nav class="blue-grey lighten-1" role="navigation">
        <div class="nav-wrapper container">
            @Html.ActionLink("Release Notes", "Index", "Home", null, new { @class = "brand-logo", id = "logo-container" })
            <ul class="right hide-on-med-and-down">
                <li>@Html.ActionLink("Dashboard", "Index", "Home", null, new {@class = "navbar-brand"})</li>
            </ul>
            <ul id="nav-mobile" class="side-nav" style="left: -250px;">
                <li>@Html.ActionLink("Dashboard", "Index", "Home", null, new { @class = "navbar-brand" })</li>
            </ul>
            <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
        </div>
    </nav>
<div class="container">
    @RenderBody()

</div>

    

@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/materialize")
    @RenderSection("scripts", required: false)
</body>
</html>
